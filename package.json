{
  "name": "tryoutgo",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:dev": "docker-compose up -d postgres redis",
    "docker:dev:logs": "docker-compose logs -f postgres redis",
    "docker:dev:down": "docker-compose down",
    "docker:dev:clean": "docker-compose down -v --remove-orphans",
    "docker:full": "docker-compose up -d",
    "docker:build": "docker-compose build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "setup:dev": "npm run docker:dev && sleep 10 && npm run db:push && npm run db:seed"
  },
  "author": "",
  "license": "ISC",
  "description": "Express.js API with Prisma ORM and Docker",
  "dependencies": {
    "express": "^5.1.0",
    "@prisma/client": "^5.7.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "prisma": "^5.7.1",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
